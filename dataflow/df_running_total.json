{
	"name": "df_running_total",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "aggregate2_sql",
						"type": "DatasetReference"
					},
					"name": "History"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "CurrentJoinHistory"
				},
				{
					"name": "SelectColumnsHistory"
				},
				{
					"name": "SelectColumnsCurrent"
				}
			],
			"scriptLines": [
				"source(output(",
				"          fact_id as short,",
				"          date_sold as date,",
				"          product as string,",
				"          color as string,",
				"          amount as float",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> History",
				"SelectColumnsCurrent, SelectColumnsHistory join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> CurrentJoinHistory",
				"History select(mapColumn(",
				"          fact_id_history = fact_id,",
				"          amount_history = amount",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectColumnsHistory",
				" select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectColumnsCurrent"
			]
		}
	}
}