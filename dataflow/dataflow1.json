{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "factsales01_sql",
						"type": "DatasetReference"
					},
					"name": "factsales01sql"
				},
				{
					"dataset": {
						"referenceName": "dimtime_sql",
						"type": "DatasetReference"
					},
					"name": "dimtimesql"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Id as long,",
				"          TimeId as long,",
				"          ProdId as long,",
				"          CustId as long,",
				"          Amount as decimal(19,4)",
				"     ),",
